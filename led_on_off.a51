ORG 0000H
BACK:    
      MOV A,#0FFH
      MOV P0,A
	  ACALL DELAY
	  MOV A,#00H
	  MOV P0,A
	  ACALL DELAY
	  SJMP BACK
	  
DELAY:
     MOV R7,#100D
L2:  MOV TMOD,#01H
     MOV TH0,#0DBH
	 MOV TL0,#0FFH
	 SETB TCON.4
L1:  JNB TCON.5,L1
     CLR TCON.4
	 CLR TCON.5
	 DJNZ R7,L2
	 RET
	 
END	 
